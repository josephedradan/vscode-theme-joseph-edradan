		"breadcrumb.focusForeground": "#ffcc00",  // This is when you move with your mouse
		"breadcrumb.foreground": "#cccccc",
		"breadcrumbPicker.background": "#222222",

		// Buttons found in the sub tabs of the side bar
		"button.background": "#222222", // TAGS: BACKGROUND 2
		"button.hoverBackground": "#333333", // TAGS: HOVER COLOR

		"debugToolBar.background": "#191936",
		"debugToolBar.border": "#191936",
		"dropdown.background": "#000000",
		"dropdown.border": "#999999",
		"editor.background": "#000000",
		"editor.findMatchBackground": "#00FF001A", // Green at 10% opacity // Search match selected background
		"editor.findMatchBorder": "#00FF00CC", // Green at 80% opacity // Search match selected border
		"editor.findMatchHighlightBackground": "#ff000040", // Red at 25% opacity // Search match background
		"editor.findMatchHighlightBorder": "#ff0000CC", // Red at 80% opacity // Search match border
		"editor.findRangeHighlightBackground": "#3a3a3a", // IDK
		"editor.foreground": "#ffffff",
